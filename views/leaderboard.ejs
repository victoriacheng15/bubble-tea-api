<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/style.css" />
    <title>leaderboard</title>
  </head>
  <body>
    <header>
      <h1>bubble tea leaderboard</h1>
      <nav>
        <ul>
          <li class="link"><a href="/">Home</a></li>
          <li class="link"><a href="/history">history</a></li>
          <li class="link"><a href="/leaderboard">leaderboard</a></li>
        </ul>
      </nav>
    </header>
    <main>
      <!--  -->
      <ul class="leaderboard">
        <% orders.forEach(order => { %>
        <li class="leaderboard-list" date-count="<%= order.count %>">
          <span><%= order.tea %> with <%= order.topping %></span>
          <span><%= order.count %></span>
        </li>
        <% }) %>
      </ul>
    </main>
  </body>
</html>
